{
    "name": "react-ducks",
    "version": "0.0.0",
    "description": "ðŸ¦† React ducks without Redux",
    "author": "iamogbz",
    "license": "Unlicense",
    "repository": {
        "type": "git",
        "url": "https://github.com/iamogbz/react-ducks"
    },
    "bugs": {
        "url": "https://github.com/iamogbz/react-ducks/issues"
    },
    "homepage": "https://github.com/iamogbz/react-ducks#readme",
    "keywords": [
        "react",
        "redux",
        "react-context",
        "ducks"
    ],
    "engines": {
        "node": ">=8",
        "npm": ">=6"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist",
        "typings"
    ],
    "scripts": {
        "build": "webpack",
        "bundlewatch": "npm pack && bundlewatch",
        "clean": "rm -rf ./dist",
        "commit": "git-cz",
        "dedupe": "npm dedupe",
        "e2e": "jest -c ./jest.e2e.config.ts",
        "lint": "eslint . --max-warnings=0 --ext .js,.jsx,.ts,.tsx",
        "release": "semantic-release",
        "test": "jest",
        "testd": "tsc --noEmit --project tsconfig.typings.json",
        "typed": "tsc --noEmit"
    },
    "release": {
        "plugins": [
            "@semantic-release/commit-analyzer",
            "@semantic-release/release-notes-generator",
            "@semantic-release/npm",
            "@semantic-release/github"
        ]
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    },
    "lint-staged": {
        "*.(j|t)s{,x}": [
            "npm run lint",
            "bash -c 'npm run typed'",
            "npm test -- --bail --findRelatedTests"
        ]
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
            "pre-commit": "lint-staged"
        }
    },
    "bundlewatch": {
        "files": [
            {
                "compression": "none",
                "maxSize": "15kb",
                "path": "react-ducks-*.tgz"
            }
        ]
    },
    "dependencies": {
        "immer": "^8.0.1"
    },
    "peerDependencies": {
        "react": "^16.0.0||^17.0.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^12.0.0",
        "@commitlint/config-conventional": "^12.0.0",
        "@commitlint/travis-cli": "^12.0.0",
        "@testing-library/react": "^11.2.5",
        "@testing-library/react-hooks": "^5.0.3",
        "@types/jest": "^26.0.20",
        "@types/node": "^14.14.31",
        "@types/react": "^17.0.2",
        "@types/react-is": "^17.0.0",
        "@typescript-eslint/eslint-plugin": "^4.15.2",
        "@typescript-eslint/parser": "^4.15.2",
        "acorn": "^8.0.5",
        "bundlewatch": "^0.3.2",
        "commitizen": "^4.2.3",
        "coveralls": "^3.1.0",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^7.20.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-config-prettier": "^8.1.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jest": "^24.1.5",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-plugin-react": "^7.22.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "expect-type": "^0.11.0",
        "husky": "^5.1.3",
        "jest": "^26.6.3",
        "lint-staged": "^10.5.4",
        "prettier": "^2.2.1",
        "prettier-eslint": "^12.0.0",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-test-renderer": "^17.0.1",
        "rxjs": "^6.6.6",
        "semantic-release": "^17.4.0",
        "stylelint": "^13.11.0",
        "stylelint-config-standard": "^20.0.0",
        "stylelint-config-styled-components": "^0.1.1",
        "stylelint-processor-styled-components": "^1.10.0",
        "ts-jest": "^26.5.2",
        "ts-node": "^9.1.1",
        "type-plus": "^3.10.3",
        "typescript": "^4.2.2",
        "webpack": "^5.24.2",
        "webpack-cli": "^4.5.0",
        "webpack-compiler-plugin": "^1.1.5"
    }
}
